#include<stdio.h>
struct food
{
char name[20]
int price;
}; 
int main()
{
struct food byte[5];
int i,x; 
for(i=0;i<5;i++)
{
printf("enter food name and price"\n);
scanf("%s",byte[i].name);
scanf("%d",&byte[i].price);
}
Printf("enter the number"\n);
scanf("%d",&x);
for(i=0;i<5;i++)
{
if(i==(x-1))
{
printf("food item -%s\n",byte);
Printf("price -%d\n", byte[i]);
break;
}
}
return 0;
}
